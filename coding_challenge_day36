class Solution {
  public:
    void bubbleSort(vector<int>& arr) {
        // code here
        int n=arr.size();
        for(int i=n-1;i>=0;i--){
            int swap1=0;
            for(int j=0;j<i;j++){
                if(arr[j]>arr[j+1]){
                    swap(arr[j],arr[j+1]);
                    swap1=1;
                }
            }
            if(swap1==0) break;
        }
    }
};
