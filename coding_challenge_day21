class Solution {
public:
    string longestCommonPrefix(vector<string>& strs) {
        sort(strs.begin(),strs.end());
        string a=strs[0];
        int n=strs.size();
        string b=strs[n-1];
        int i=0;
        for( i=0;i<a.size();i++){
            if(a[i]!=b[i]) break;
        }
        return strs[0].substr(0,i);
    }
};
